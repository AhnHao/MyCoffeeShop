<nav aria-label="Page navigation example">
    <ul class="pagination justify-content-center">
        <li class="page-item <%= currentPage === 1 ? 'disabled' : '' %>">
            <a class="page-link p-3" href="?page=<%= currentPage - 1 %>" aria-label="Previous">
                <span aria-hidden="true">&laquo;</span>
            </a>
        </li>
        <% if(currentPage !== 1 && previousPage !== 1) { %>
            <li class="page-item"><a class="page-link p-3" href="?page=1">1</a></li>
        <% } %>
        <% if(currentPage > 3) {%>
            <li class="page-item disabled"><a class="page-link p-3" href="#">...</a></li>
        <% } %>
        <% if(hasPreviousPage) { %>
            <li class="page-item"><a class="page-link p-3" href="?page=<%= previousPage %>"><%= previousPage %></a></li>
        <% } %>
        <li class="page-item active"><a class="page-link p-3" href="?page=<%= currentPage %>"><%= currentPage %></a></li>
        <% if(hasNextPage) { %>
            <li class="page-item"><a class="page-link p-3" href="?page=<%= nextPage %>"><%= nextPage %></a></li>
        <% } %>
        <% if(currentPage + 2 < lastPage) {%>
            <li class="page-item disabled"><a class="page-link p-3" href="#">...</a></li>
        <% } %>
        <% if(lastPage !== currentPage && lastPage !== nextPage) {%>
            <li class="page-item"><a class="page-link p-3" href="?page=<%= lastPage %>"><%= lastPage %></a></li>
        <% } %>
        <li class="page-item <%= currentPage === lastPage ? 'disabled' : '' %>">
            <a class="page-link p-3" href="?page=<%= currentPage + 1 %>" aria-label="Next">
            <span aria-hidden="true">&raquo;</span>
            </a>
        </li>
    </ul>
</nav>